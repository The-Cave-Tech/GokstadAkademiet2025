{
  "collectionName": "components_shop_cart_items",
  "info": {
    "displayName": "Cart Item",
    "description": "Items in the shopping cart",
    "icon": "shopping-cart"
  },
  "options": {},
  "attributes": {
    "product": {
      "type": "relation",
      "relation": "oneToOne",
      "target": "api::product.product"
    },
    "quantity": {
      "type": "integer",
      "required": true,
      "min": 1,
      "default": 1
    },
    "price": {
      "type": "decimal",
      "required": true,
      "min": 0,
      "default": 0
    },
    "subtotal": {
      "type": "decimal",
      "required": true,
      "min": 0,
      "default": 0
    }
  }
}